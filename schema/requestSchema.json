{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "timezones"
  ],
  "properties": {
    "timezones": {
      "$id": "#/properties/timezones",
      "type": "array",
      "title": "The Timezones Schema",
      "items": {
        "$id": "#/properties/timezones/items",
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "Europe/Moscow",
          "Asia/Tokio"
        ],
        "pattern": "^(.*)$"
      }
    }
  }
}